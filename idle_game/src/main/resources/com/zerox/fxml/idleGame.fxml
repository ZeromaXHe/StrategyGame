<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.control.Button?>
<AnchorPane fx:id="gameAnchor" prefHeight="600" prefWidth="800" xmlns="http://javafx.com/javafx/10.0.2-internal"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.zerox.controller.MainController">
    <ProgressBar AnchorPane.topAnchor="100" AnchorPane.leftAnchor="100" fx:id="progressBar"/>
    <Button AnchorPane.topAnchor="150" AnchorPane.leftAnchor="100" fx:id="button" text="button" onAction="#onButtonClick"/>
</AnchorPane>